{
  "crons": [
    {
      "path": "/api/cron/sync-salla-reviews",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/cron/webhook-retry",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/backfill-review-ids",
      "schedule": "*/10 * * * *"
    }
  ],
  "headers": [
    {
      "source": "/widgets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}